{
  "family": "wildfly",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "wildfly-duke",
      "image": "495913029085.dkr.ecr.eu-west-3.amazonaws.com/duke-ecr",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        },
        {
          "containerPort": 9990,
          "hostPort": 9990,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}