{
  "family": "duke",
  "containerDefinitions": [
    {
      "name": "wildfly",
      "image": "495913029085.dkr.ecr.eu-west-3.amazonaws.com/duke-ecr",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        },
        {
          "containerPort": 9990,
          "hostPort": 9990,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "healthCheck": {
        "retries": 3,
        "command": [
          "CMD-SHELL",
          "curl -f http://localhost:8080/ || exit 1"
        ],
        "timeout": 5,
        "interval": 30
      }
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512",
  "networkMode": "awsvpc"
}